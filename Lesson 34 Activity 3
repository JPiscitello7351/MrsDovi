import java.util.Scanner;

class Lesson_34_Activity_Three {
  
   public static int findMax(int a[])
    {
     int max = a[0];

     for (int i = 1; i < a.length; i++) {
       if (a[i] > max) {
         max = a[i];
       }
     }
     return max;
   }
  
    public static void main(String[] args)
    {
      int a[] = {55, 45, 62};
      
      System.out.println(findMax(a));
    }
}
