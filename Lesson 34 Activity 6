import java.util.Scanner;

class Lesson_34_Activity_Six {
  
  public static boolean allPositive(int [] a)
    {
     for (int i = 0; i < a.length; i++)
       if (a[i] < 0)
       return false;
     
     return true; 
    }
  
    public static void main(String[] args)
     {
      int a[] = {-55, 53, -23};
      
      System.out.println("all are positive?: " + allPositive(a));
    }
}
