import java.util.Scanner;

class Lesson_34_Activity_Four {
  
   public static int findMin(int [] a)
    {
     int min = a[0];

     for (int i = 1; i < a.length; i++) {
       if (a[i] < min) {
         min = a[i];
       }
     }
     return min;
    }
  
    public static void main(String[] args)
     {
      int a[] = {55, 45, 62};
      
      System.out.println(findMin(a));
    }
}
